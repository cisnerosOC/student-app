<h1><%= t("index") %></h1>

<table class="table">
  <tr>
    <th>Id</th>
    <th><%= t("first_name") %></th>
    <th><%= t("last_name") %></th>
    <th><%= t("action") %></th>
  </tr>
  <%@students.each do |student| %>
  <tr>
    <td><%= student.id %></td>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><a href="/students/<%= student.id %>/edit"><button type="button" class="btn btn-primary"><%= t("update_btn") %></button></a></td>
  </tr>
  <%end  %>
</table>


